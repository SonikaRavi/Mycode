#include <iostream>
#include<cmath>
using namespace std;

int main() {
	// your code goes here
	int num1,num2;
	char sy;
	cin>>num1;
	cin>>num2;
	cin>>sy;
	switch(sy)
	{
		case '+':cout<<num1+num2;
		break;
		case '-':cout<<abs(num1-num2);
		break;
		case '*':cout<<(num1*num2);
		break;
		case '%':cout<<(num1%num2);
		break;
	}
	return 0;
}
